<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="pos_inicis.OrderReceipt" t-inherit="point_of_sale.OrderReceipt" t-inherit-mode="extension" owl="1">
        <xpath expr="//t[@t-foreach='receipt.paymentlines']" position="inside">
            <t t-if="line.transaction_id">
                <div class="pos-receipt-left-padding">
                    <span>APPROVAL No.: </span>
                    <span>
                        <t t-esc="line.transaction_id" />
                    </span>
                </div>
            </t>
        </xpath>
    </t>

</templates>
