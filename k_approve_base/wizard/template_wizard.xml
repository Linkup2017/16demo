<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="view_kapprove_template_wizard" model="ir.ui.view">
            <field name="name">kapprove.template.wizard.view</field>
            <field name="model">kapprove.template.wizard</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="templ_type"/>
                        <field name="template"/>
                    </group>

                    <footer>
                        <!--<button name="action_template" string="OK" type="object" class="oe_highlight"/>-->
                        <button string="Cancel" class="btn btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>