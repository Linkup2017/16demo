<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="my_approval_sequence" model="ir.sequence">
            <field name="name">My Approvals</field>
            <field name="code">kapprove.doc</field>
            <field name="prefix">A</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record
                id="model_kapprove_doc_pdf"
                model="ir.actions.server"
            >
            <field name="name">Print PDF</field>
            <field name="model_id" ref="k_approve_base.model_kapprove_doc" />
            <field name="binding_model_id" ref="k_approve_base.model_kapprove_doc" />
            <field name="binding_view_types">list</field>
            <field name="state">code</field>
            <field name="code">action = records.action_pdf()</field>
        </record>

    </data>
</odoo>
