<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="account_card_number_view_tree" model="ir.ui.view">
        <field name="name">account_card_number_tree_view</field>
        <field name="model">account.card.no</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="card_code"/>
            </tree>
        </field>
    </record>

    <record id="account_card_number_view_form" model="ir.ui.view">
        <field name="name">account_card_number_form_view</field>
        <field name="model">account.card.no</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="card_code"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>


    <record id="move_card_list" model="ir.actions.act_window">
       <field name="name">Account Card Number List</field>
        <field name="type">ir.actions.act_window</field>
       <field name="res_model">account.card</field>
       <field name="view_mode">tree,form</field>
    </record>


