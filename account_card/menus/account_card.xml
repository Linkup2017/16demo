<odoo>

    <record id="account_card_import_update" model="ir.cron">
        <field name="name">Account Card Import Update</field>
        <field name="model_id" ref="model_account_card_receipts"/>
        <field name="state">code</field>
        <field name="code">model.action_import_receipt_item()</field>
        <field name="active" eval="True" />
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">5</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field eval="False" name="doall" />
    </record>
</odoo>
