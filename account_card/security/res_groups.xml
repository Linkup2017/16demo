<odoo noupdate="0">

        <record id="module_category_account_card" model="ir.module.category">
            <field name="name">Account Card</field>
            <field name="sequence">21</field>
        </record>

        <record id="group_account_card_user" model="res.groups">
            <field name="name">Account Card User</field>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]" />
            <field name="category_id" ref="module_category_account_card" />
        </record>

        <record id="group_account_card_manager" model="res.groups">
            <field name="name">Account Card Manager</field>
            <field name="category_id" ref="module_category_account_card" />
            <field name="implied_ids" eval="[(4, ref('group_account_card_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>


        </record>
<data noupdate="1">

    <record model="ir.rule" id="account_card_comp_rule">
        <field name="name">Account Card Comp Rule</field>
        <field name="model_id" ref="model_account_card"/>
        <field name="domain_force">['|',('company_id','=',False),('company_id', 'in', company_ids)]</field>
    </record>

    <record model="ir.rule" id="account_card_receipts_comp_rule">
        <field name="name">Account Card Receipts Comp Rule</field>
        <field name="model_id" ref="model_account_card_receipts"/>
        <field name="domain_force">['|',('company_id','=',False),('company_id', 'in', company_ids)]</field>
    </record>


</data>

</odoo>
